<Window x:Class="SistemaSECI.VentanaUsuarios"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SistemaSECI"
        mc:Ignorable="d"
        Title="Usuarios" 
        Height="228" 
        Width="340"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen" >
    <Grid>
        <Button 
            x:Name="botonUsuarioNuevo_VUsuarios" 
            Content="Nuevo" 
            HorizontalAlignment="Left" 
            VerticalAlignment="Top" 
            Width="110" 
            Background="White" 
            Margin="40,75,0,0" 
            Height="60" 
            Click="botonUsuarioNuevo_VUsuarios_Click" TabIndex="0" 
            />
        <Button 
        	x:Name="botonUsuarioExistente_VUsuarios" 
        	Content="Reciente" 
        	HorizontalAlignment="Left" 
        	VerticalAlignment="Top" 
        	Width="110" 
        	Background="White" 
        	Margin="190,75,0,0" 
        	Height="60" 
            Click="botonUsuarioExistente_VUsuarios_Click" TabIndex="1" 
        	/>
    </Grid>
</Window>
