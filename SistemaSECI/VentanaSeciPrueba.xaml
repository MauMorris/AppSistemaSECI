<Window x:Class="SistemaSECI.VentanaSeciPrueba"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SistemaSECI"
        mc:Ignorable="d"
        Icon="logoUNAM.jpg"
        WindowStartupLocation="CenterScreen"
        Title="SECI" 
        Height="300" 
        Width="300">
    <Grid>
        <Button 
            x:Name="regresarBoton_VLogros"
            Content="REGRESAR"
            Margin="40,44,0,0" 
        	VerticalAlignment="Top" 
            Background="White"
        	Width="80" 
        	Height="45"
            HorizontalAlignment="Left" Click="regresarBoton_VLogros_Click" 
            />
        <Button 
        	x:Name="okBoton_VSeci"
        	Content="Ok"
        	HorizontalAlignment="Left" 
        	VerticalAlignment="Top" 
        	Width="186" 
        	Margin="56,169,0,0"
        	Background="White"
        	ClickMode="Press" 
        	Height="50" 
        	TabIndex="8" 
            Click="okBoton_VSeci_Click" 
            />

    </Grid>
</Window>
